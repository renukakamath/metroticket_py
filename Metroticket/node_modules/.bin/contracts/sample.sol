pragma solidity >= 0.5.0 < 0.7.0;
contract metro{
 
struct transaction{

	// State variables
	uint transaction_id;
	uint request_id;
	string amount;
	string fromplace;
	string toplace;
	string date;
	string status;
	string travelstatus;
	

}

 
transaction []m;



function add_transaction(uint transaction_id,uint request_id,string memory amount,string memory fromplace,string memory toplace,string memory date,string memory status,string memory travelstatus) public{
	transaction memory m1=transaction(transaction_id,request_id,amount,fromplace,toplace,date,status,travelstatus);
	m.push(m1);
}





struct transactions{

	// State variables
	uint transaction_id;
	uint request_id;
	string amount;
	string fromplace;
	string toplace;
	string date;
	string status;
	string travelstatus;
	string total;
	

}

 
transactions []t;



function add_transactions(uint transaction_id,uint request_id,string memory amount,string memory fromplace,string memory toplace,string memory date,string memory status,string memory travelstatus,string memory total) public{
	transactions memory t1=transactions(transaction_id,request_id,amount,fromplace,toplace,date,status,travelstatus,total);
	t.push(t1);
}





}




